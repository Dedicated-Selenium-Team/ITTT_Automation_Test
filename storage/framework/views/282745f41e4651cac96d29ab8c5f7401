<script type="text/javascript">
$.ajaxSetup({
  headers : {
    'X-CSRF-TOKEN' : $('meta[name = "_token"]').attr('contents')
  }
});
$('#project-designation').on('submit',function(e){
  e.preventDefault();
  var value = $('#select_project :selected').text();
  var value1 = $('#select_designation').val();
  $.ajax({
    type : 'get',
    url :$('#project-designation').attr('action') + '/' + value + '/' + value1,
    data : {
      'name':value,
      'id':value1,
      "_token": "<?php echo e(csrf_token()); ?>"
    },
    success: function(data) {
      console.log(data);
    }
  });
});
</script>
